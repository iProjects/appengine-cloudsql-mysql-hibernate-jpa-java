<?xml version="1.0" encoding="utf-8"?>
<web-app
        version="2.5"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  
    <servlet>
        <servlet-name>makeoffer</servlet-name>
        <servlet-class>com.google.appengine.demos.MakeOffersServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>makeoffer</servlet-name>
        <url-pattern>/makeoffer</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>myoffers</servlet-name>
        <servlet-class>com.google.appengine.demos.ListMyOffersServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>myoffers</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
